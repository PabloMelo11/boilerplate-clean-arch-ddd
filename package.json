{
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "boilerplate",
    "main": "index.js",
    "scripts": {
        "build": "tsc && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
        "copy:doc": "cp -r ./src/infra/http/routes/doc/definitionsData ./dist/infra/http/routes/doc/",
        "test": "node ./node_modules/.bin/jest --verbose --passWithNoTests",
        "build-test": "npm run build && git diff HEAD --quiet && npm run test --passWithNoTests",
        "prepare:husk": "husky install",
        "lint-staged": "lint-staged",
        "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/infra/server.ts",
        "start": "node dist/infra/server.js",
        "typeorm:clean-database": "npm run typeorm schema:drop && npm run typeorm migration:run",
        "typeorm:run": "npm run typeorm migration:run",
        "typeorm:generate": "npm run typeorm migration:generate -- -n",
        "typeorm:drop:database": "npm run typeorm schema:drop",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
    },
    "keywords": [],
    "author": "Pablo Santos",
    "contributors": [
        "Pablo Santos"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/node": "^7.15.8",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-decorators": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-typescript": "^7.12.7",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/pg": "^7.14.11",
        "@types/sinon": "^10.0.13",
        "@types/supertest": "^2.0.10",
        "@types/swagger-jsdoc": "^3.0.2",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/uuid": "^8.3.4",
        "@types/yup": "^0.29.8",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.2.1",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^8.0.1",
        "jest": "27.0.6",
        "lint-staged": "^13.0.3",
        "prettier": "^2.1.2",
        "sinon": "^14.0.0",
        "supertest": "^5.0.0",
        "ts-jest": "27.0.5",
        "ts-node": "10.8.1",
        "ts-node-dev": "^1.1.6",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@seidor-cloud-produtos/lib-seidor-common": "^0.0.8",
        "@seidor-cloud-produtos/typeorm": "0.0.4",
        "aws-sdk": "^2.1210.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "kafkajs": "^1.15.0",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^4.3.0",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.28",
        "uuid": "^9.0.0",
        "yup": "^0.29.3"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix --ext .ts .",
            "npm run test --passWithNoTests --findRelatedTests --allow-empty"
        ]
    }
}
